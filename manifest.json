{
  "name": "Structutal Poem Writer",
  "description": "write a poem about the html structure and the text of a page",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Generate Poem from Html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content-script.js"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "storage"
  ]
}
